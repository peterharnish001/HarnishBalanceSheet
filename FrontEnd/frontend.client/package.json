    "@angular/forms": "^21.1.0",
    "@angular/material": "^21.1.5",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "datatables.net": "^2.3.7",
    "datatables.net-dt": "^2.3.7",
    "jest-editor-support": "*",
    "jquery": "^4.0.0",
    "jqwidgets-ng": "^25.0.2",
    "ngx-spinner": "^21.0.0",
    "ngx-toastr": "^20.0.5",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
